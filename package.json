{
    "name": "@illinois-toolkit/ilw-columns",
    "description": "Illinois Toolkit: This uses the CSS flexbox to split the container into multiple sections.",
    "repository": "github:web-illinois/ilw-columns",
    "private": false,
    "license": "MIT",
    "version": "1.2.2",
    "type": "module",
    "files": [
        "src/**",
        "dist/**",
        "builder/**"
    ],
    "exports": {
        ".": {
            "import": "./dist/ilw-columns.js",
            "require": "./dist/ilw-columns.cjs",
            "default": "./dist/ilw-columns.js"
        },
        "./ilw-modal.css": {
            "import": "./dist/ilw-columns.css"
        }
    },
    "main": "./dist/ilw-columns.js",
    "module": "./dist/ilw-columns.js",
    "scripts": {
        "dev": "vite",
        "build": "tsc --noEmit && vite build --config vite.transpile.config.ts --emptyOutDir && vite build --config vite.build.config.ts --emptyOutDir",
        "preview": "vite preview",
        "prepack": "npm run build",
        "test:axe": "playwright test",
        "test:axe:github": "playwright test --config=playwright.ci.config.ts"
    },
    "dependencies": {
        "lit": "3.1.3"
    },
    "devDependencies": {
        "@illinois-toolkit/ilw-core": "^1.0.5",
        "@playwright/test": "^1.56.0",
        "playwright": "^1.56.0",
        "typescript": "^5.9.3",
        "vite": "^7.1.10",
        "vite-plugin-dts": "^4.5.4"
    }
}
